# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET     /                           controllers.Application.index

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.at(path="/public", file)


GET /mobileApplications             controllers.MobileApplications.list
GET /mobileApplications/:id         controllers.MobileApplications.show(id: Long)
GET /mobileApplications/:id/edit    controllers.MobileApplications.edit(id: Long)
PUT /mobileApplications/:id         controllers.MobileApplications.update(id: Long)

GET /privacyPolicy                  controllers.Pages.privacyPolicy()

GET /about                          controllers.Pages.about()
